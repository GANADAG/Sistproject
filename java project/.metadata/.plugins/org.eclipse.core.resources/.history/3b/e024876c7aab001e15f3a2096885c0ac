package day0105;

public class IfTest_009 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=10;
		
		if(a>5) {
			System.out.println(a+"는 5보다 크다");
		}
		
		System.out.println("if문 종료");
		
		
		int b=9;
		
		if(b%2==0)
			System.out.println(b+"는 짝수이다");
		else 
		   System.out.println(b+"는 홀수이다");
		System.out.println("if문 종료");
		
	}

}
